{
  "manifest_version": 3,
  "name": "Discord Webhook Extension",
  "version": "1.0",
  "description": "A Chrome extension for sending data to Discord webhooks.",
  "icons": {
    "16": "./img16.png",
    "48": "./img48.png",
    "128": "./img128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "./img16.png"
    }
  },
  "permissions": ["clipboardRead", "clipboardWrite"],
  "default_permission": ["clipboardRead", "clipboardWrite"]
}
